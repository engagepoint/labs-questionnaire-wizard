<?xml version="1.0" encoding="UTF-8"?>
<questionnaire-forms
        xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xsi:noNamespaceSchemaLocation='jaxb/super_binding/XSDforWizard.xsd'>
    <questionnaire-form form-id="testController">
        <form-name>TestController</form-name>
        <pages>
            <page page-number="1" page-id="formPage1">
                <groups-of-questions>
                    <group group-id="page1Group1">
                        <group-name>Topic 1 of First Page</group-name>
                        <questions>
                            <question answer-required="false" question-id="pageOneGroupOne">
                                <question-title>Question one</question-title>
                                <question-type>TEXT</question-type>
                                <help-text>Type yes if it's topic one of first page?</help-text>
                                <dependent-questions>
                                    <dependent-question/>
                                </dependent-questions>
                                <options>
                                    <option></option>
                                </options>
                                <range/>
                                <grid>
                                    <columns>
                                        <column></column>
                                    </columns>
                                    <rows>
                                        <row></row>
                                    </rows>
                                </grid>
                                <default-answers>
                                    <default-answer>Default answer</default-answer>
                                </default-answers>
                            </question>
                       </questions>
                    </group>
            </groups-of-questions>
            </page>
        </pages>
    </questionnaire-form>
</questionnaire-forms>