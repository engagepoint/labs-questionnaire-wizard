<?xml version="1.0" encoding="UTF-8"?>
<questionnaire-forms
        xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xsi:noNamespaceSchemaLocation='jaxb/super_binding/XSDforWizard.xsd'>
    <questionnaire-form form-id="form_for_test">
        <form-name>Test All Components</form-name>
        <pages>
            <page page-number="1" page-id="page_one">
                <page-name>Page One</page-name>
                <groups-of-questions>
                    <group group-id="text_topic">
                        <group-name>Text</group-name>
                        <questions>
                            <question answer-required="false" question-id="text_question">
                                <question-title>Text Question</question-title>
                                <question-type>TEXT</question-type>
                                <help-text>Text Question</help-text>
                                <default-answers>
                                    <default-answer>Default text answer</default-answer>
                                </default-answers>
                            </question>
                        </questions>
                    </group>
                    <group group-id="paragraphtext_topic">
                        <group-name>Paragraph Text</group-name>
                        <questions>
                            <question answer-required="false" question-id="paragraphtext_question">
                                <question-title>Paragraph Text Question</question-title>
                                <question-type>PARAGRAPHTEXT</question-type>
                                <help-text>Paragraph Text Question</help-text>
                                <default-answers>
                                    <default-answer>Default paragraph text answer</default-answer>
                                </default-answers>
                            </question>
                        </questions>
                    </group>
                    <group group-id="multiplechoice_topic">
                        <group-name>Multiple Choice</group-name>
                        <questions>
                            <question answer-required="false" question-id="multiplechoice_question">
                                <question-title>Multiple Choice Question</question-title>
                                <question-type>MULTIPLECHOICE</question-type>
                                <help-text>Multiple Choice Question</help-text>
                                <options>
                                    <option>Option 1</option>
                                    <option>Option 2</option>
                                    <option>Option 3</option>
                                </options>
                                <default-answers>
                                    <default-answer>Option 2</default-answer>
                                </default-answers>
                            </question>
                        </questions>
                    </group>
                    <group group-id="checkboxes_topic">
                        <group-name>Checkboxes</group-name>
                        <questions>
                            <question answer-required="false" question-id="checkboxes_question">
                                <question-title>Checkboxes Question</question-title>
                                <question-type>CHECKBOX</question-type>
                                <help-text>Checkboxes Question</help-text>
                                <options>
                                    <option>Option 1</option>
                                    <option>Option 2</option>
                                    <option>Option 3</option>
                                </options>
                                <default-answers>
                                    <default-answer>Option 2</default-answer>
                                    <default-answer>Option 3</default-answer>
                                </default-answers>
                            </question>
                        </questions>
                    </group>
                    <group group-id="choosefromlist_topic">
                        <group-name>Choose From List</group-name>
                        <questions>
                            <question answer-required="false" question-id="choosefromlist_question">
                                <question-title>Choose From List Question</question-title>
                                <question-type>CHOOSEFROMLIST</question-type>
                                <help-text>Choose From List Question</help-text>
                                <options>
                                    <option>Option 1</option>
                                    <option>Option 2</option>
                                    <option>Option 3</option>
                                </options>
                            </question>
                        </questions>
                    </group>
                    <group group-id="range_topic">
                        <group-name>Range</group-name>
                        <questions>
                            <question answer-required="false" question-id="range_question">
                                <question-title>Range Question</question-title>
                                <question-type>RANGE</question-type>
                                <help-text>Range Question</help-text>
                                <range range-begin="30" range-end="70" />
                                <default-answers>
                                    <default-answer>44</default-answer>
                                    <default-answer>55</default-answer>
                                </default-answers>
                            </question>
                        </questions>
                    </group>
                    <group group-id="grid_topic">
                        <group-name>Grid</group-name>
                        <questions>
                            <question answer-required="false" question-id="grid_question">
                                <question-title>Grid Question</question-title>
                                <question-type>GRID</question-type>
                                <help-text>Grid Question</help-text>
                                <grid>
                                    <columns>
                                        <column>Col 1</column>
                                        <column>Col 2</column>
                                        <column>Col 3</column>
                                    </columns>
                                    <rows>
                                        <row>Row 1</row>
                                        <row>Row 2</row>
                                    </rows>
                                </grid>
                                <grid-oneInCol>true</grid-oneInCol>
                                <grid-oneInRow>true</grid-oneInRow>
                                <default-answers>
                                    <default-answer>true,false,false</default-answer>
                                    <default-answer>false,true,false</default-answer>
                                </default-answers>
                            </question>
                        </questions>
                    </group>
                    <group group-id="date_topic">
                        <group-name>Date</group-name>
                        <questions>
                            <question answer-required="false" question-id="date_question">
                                <question-title>Date Question</question-title>
                                <question-type>DATE</question-type>
                                <help-text>Date Question</help-text>
                                <default-answers>
                                    <default-answer>07-07-2007</default-answer>
                                </default-answers>
                            </question>
                        </questions>
                    </group>
                    <group group-id="time_topic">
                        <group-name>Time</group-name>
                        <questions>
                            <question answer-required="false" question-id="time_question">
                                <question-title>Time Question</question-title>
                                <question-type>TIME</question-type>
                                <help-text>Time Question</help-text>
                                <default-answers>
                                    <default-answer>09:15</default-answer>
                                </default-answers>
                            </question>
                        </questions>
                    </group>
                    <group group-id="fileupload_topic">
                        <group-name>File Upload</group-name>
                        <questions>
                            <question answer-required="false" question-id="fileupload_question">
                                <question-title>File Upload Question</question-title>
                                <question-type>FILEUPLOAD</question-type>
                                <help-text>File Upload Question</help-text>
                            </question>
                        </questions>
                    </group>
                </groups-of-questions>
            </page>
            <page page-number="2" page-id="page_two">
                <page-name>Page Two</page-name>
                <groups-of-questions>
                    <group group-id="empty_topic">
                        <group-name>Empty Topic</group-name>
                        <questions>
                        </questions>
                    </group>
                </groups-of-questions>
            </page>
        </pages>
    </questionnaire-form>
</questionnaire-forms>
